{
    "name": "prolic/humus-amqp",
    "type": "library",
    "description": "PHP-AMQP library with RabbitMQ Extensions",
    "keywords": ["rabbit", "rabbitmq", "queue", "amqp", "humus"],
    "homepage": "https://github.com/prolic/HumusAmqp",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",
        "beberlei/assert": "^2.4"
    },
    "require-dev": {
        "container-interop/container-interop": "^1.1",
        "zendframework/zend-servicemanager": "^2.6.0",
        "phpunit/phpunit": "~4.8",
        "fabpot/php-cs-fixer": "1.7.*",
        "satooshi/php-coveralls": "^1.0",
        "videlalvaro/php-amqplib": "^2.6"
    },
    "suggest": {
        "ext-pcntl": "For process control management",
        "ext-amqp": "For usage together with php amqp extension",
        "videlalvaro/php-amqplib": "For usage together with php-amqplib"
    },
    "autoload": {
        "psr-4": {
            "Humus\\Amqp\\": "src"
        }
    }
}
